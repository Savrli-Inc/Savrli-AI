{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "^/ai/chat$",
      "dest": "api/index.py"
    },
    {
      "src": "^/openapi.json$",
      "dest": "api/index.py"
    },
    {
      "src": "^/docs$",
      "dest": "api/index.py"
    },
    {
      "src": "^/docs/.*$",
      "dest": "api/index.py"
    },
    {
      "src": "^/.*$",
      "dest": "api/index.py"
    }
  ]
}
