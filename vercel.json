{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/apidocs/?",
      "dest": "api/index.py",
      "methods": ["GET"]
    },
    {
      "src": "/.*",
      "dest": "api/index.py"
    }
  ]
}
