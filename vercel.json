"routes": [
  {
    "src": "/public/(.*)",
    "dest": "/public/$1"
  },
  {
    "src": "/static/(.*)",
    "dest": "/static/$1"
  },
  {
    "src": "/ai/(.*)",
    "dest": "api/index.py"
  },
  {
    "src": "/docs",
    "dest": "api/index.py"
  },
  {
    "src": "/playground",
    "dest": "/pages/playground.html"
  },
  {
    "src": "/dashboard",
    "dest": "/pages/dashboard.html"
  },
  {
    "src": "/",
    "dest": "/public/index.html"
  },
  {
    "src": "/(.*)",
    "dest": "api/index.py"
  }
]
